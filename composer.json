{
    "name": "californiamountainsnake/php-database-entities",
    "type": "library",
    "description": "These are php data entities and utils, intended for work with the information from database in oop-style.",
    "license": "MIT",
    "authors": [
        {
            "name": "CaliforniaMountainSnake",
            "email": "CaliforniaMountainSnake1@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1|8.0",
        "ext-json": "*",
        "myclabs/php-enum": "~1.5"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CaliforniaMountainSnake\\DatabaseEntities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "\"vendor/bin/phpunit\""
        ]
    }
}
